{
	"name": "obsidian-ai-actions",
	"version": "0.2.0",
	"description": "Obsidian AI Actions - Based on AI-powered Text Editor by Zekun Shen",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
