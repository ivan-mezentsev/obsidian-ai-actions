		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.25.0",
		"esbuild-sass-plugin": "^3.3.1",
		"esbuild-svelte": "^0.8.2",
		"lucide-svelte": "^0.446.0",
		"obsidian": "latest",
		"sass": "^1.89.2",
		"svelte": "^4.2.19",
		"svelte-preprocess": "^6.0.3",
		"tslib": "2.4.0",
