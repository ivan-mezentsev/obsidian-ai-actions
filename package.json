		"@typescript-eslint/parser": "^8.0.0",
		"builtin-modules": "^4.0.0",
		"esbuild": "^0.25.0",
		"esbuild-sass-plugin": "^3.3.1",
		"esbuild-svelte": "^0.8.2",
		"eslint": "^9.0.0",
		"lucide-svelte": "^0.446.0",
		"obsidian": "latest",
		"sass": "^1.89.2",
		"svelte": "^4.2.19",
